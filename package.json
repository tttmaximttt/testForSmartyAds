{
  "name": "supernaut",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "scripts": {
    "preforever": "npm run build",
    "start": "NODE_ENV=development nodemon --exec npm run babel-node -- server.js",
    "forever": "./node_modules/.bin/forever start --append -o ./forever.log -e ./forever.log ./build/server.js",
    "forever-stop": "./node_modules/.bin/forever stop ./build/server.js",
    "status": "./node_modules/.bin/forever list",
    "babel-node": "babel-node --presets=es2015 --ignore='node_modules'",
    "preserver": "npm run build",
    "server": "NODE_ENV=development node ./build/server.js",
    "build": "babel . -d build --ignore node_modules,build"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "config": "^1.14.0",
    "dotenv": "^2.0.0",
    "forever": "^0.14.1",
    "formidable": "^1.0.17",
    "install": "^0.6.1",
    "json5": "^0.4.0",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "multer": "^1.1.0",
    "nodemon": "^1.9.1",
    "npm": "^3.8.2",
    "require-fu": "^1.0.0",
    "restify": "^4.0.4",
    "restify-cookies": "^0.2.0",
    "restify-errors": "^4.2.1",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "bunyan-prettystream": "hadfieldn/node-bunyan-prettystream",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "nock": "^7.5.0"
  },
  "babel": {
    "babelrc": true,
    "ignore": [
      "test/server.js",
      "test/**/*.js",
      "logs/**/*.json",
      "uploads/*.json",
      "*.log",
      "node_modules/*",
      "build/*"
    ]
  },
  "nodemon": {
    "verbose": true,
    "ignore": [
      "test/server.js",
      "test/**/*.js",
      "logs/**/*.json",
      "uploads/*.json",
      "*.log",
      "node_modules/*"
    ],
    "exclude": [
      "node_modules"
    ]
  },
  "presets": [
    "es2015",
    "stage-0"
  ],
  "engine": {
    "node": ">=0.10"
  }
}
